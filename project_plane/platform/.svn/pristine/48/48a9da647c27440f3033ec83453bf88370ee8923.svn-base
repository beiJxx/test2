package com.itany.platform.pojo;

public class SysRoleMenus implements java.io.Serializable
{
    
    /**
     * serialVersionUID
     */
    private static final long serialVersionUID = 1L;

    private int srmId;
    
    private SysFunctionMenus sysFunctionMenus;
    
    private SysRole sysRole;
    
    public SysRoleMenus()
    {
    }
    
    public SysRoleMenus(SysFunctionMenus sysFunctionMenus, SysRole sysRole)
    {
        this.sysFunctionMenus = sysFunctionMenus;
        this.sysRole = sysRole;
    }
    
    public int getSrmId()
    {
        return this.srmId;
    }
    
    public void setSrmId(int srmId)
    {
        this.srmId = srmId;
    }
    
    public SysFunctionMenus getSysFunctionMenus()
    {
        return this.sysFunctionMenus;
    }
    
    public void setSysFunctionMenus(SysFunctionMenus sysFunctionMenus)
    {
        this.sysFunctionMenus = sysFunctionMenus;
    }
    
    public SysRole getSysRole()
    {
        return this.sysRole;
    }
    
    public void setSysRole(SysRole sysRole)
    {
        this.sysRole = sysRole;
    }
    
}