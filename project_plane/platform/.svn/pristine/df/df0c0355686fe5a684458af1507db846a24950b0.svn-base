package com.itany.platform.pojo;

public class DistributorsLevel implements java.io.Serializable
{
    
    
    private int dlId;
    
    private String level;
    
    private double saleRate;
    
    private int share;
    
    private double margin;
    
    private boolean flag;
    
    public DistributorsLevel()
    {
    }
    
    public DistributorsLevel(String level, double saleRate, int share, boolean flag)
    {
        this.level = level;
        this.saleRate = saleRate;
        this.share = share;
        this.flag = flag;
    }
    
    public DistributorsLevel(String level, double saleRate, int share, double margin, boolean flag)
    {
        this.level = level;
        this.saleRate = saleRate;
        this.share = share;
        this.margin = margin;
        this.flag = flag;
    }
    
    
    public int getDlId()
    {
        return this.dlId;
    }
    
    public void setDlId(int dlId)
    {
        this.dlId = dlId;
    }
    
    public String getLevel()
    {
        return this.level;
    }
    
    public void setLevel(String level)
    {
        this.level = level;
    }
    
    public double getSaleRate()
    {
        return this.saleRate;
    }
    
    public void setSaleRate(double saleRate)
    {
        this.saleRate = saleRate;
    }
    
    public int getShare()
    {
        return this.share;
    }
    
    public void setShare(int share)
    {
        this.share = share;
    }
    
    public double getMargin()
    {
        return this.margin;
    }
    
    public void setMargin(double margin)
    {
        this.margin = margin;
    }
    
    public boolean getFlag()
    {
        return this.flag;
    }
    
    public void setFlag(boolean flag)
    {
        this.flag = flag;
    }
    
}